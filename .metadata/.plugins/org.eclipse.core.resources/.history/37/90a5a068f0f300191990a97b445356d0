package application;

import javafx.beans.property.IntegerProperty;
import javafx.beans.property.StringProperty;

public class Employee {
	private IntegerProperty id;
	private StringProperty name;
	private StringProperty gender;
	private StringProperty birthYear;
	private StringProperty position;
	private StringProperty area;
	public Employee(Integer id, String name, String gender, String birthYear, String position, String area) {
		super();
		this.id.set(id);
		this.name.set(name);
		this.gender = gender;
		this.birthYear = birthYear;
		this.position = position;
		this.area = area;
	}
	public IntegerProperty getId() {
		return id;
	}
	public StringProperty getName() {
		return name;
	}
	public StringProperty getGender() {
		return gender;
	}
	public StringProperty getBirthYear() {
		return birthYear;
	}
	public StringProperty getPosition() {
		return position;
	}
	public StringProperty getArea() {
		return area;
	}
	
	
}
