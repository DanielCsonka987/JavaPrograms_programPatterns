package application;

import javafx.beans.property.IntegerProperty;
import javafx.beans.property.StringProperty;

public class Employee {
	private IntegerProperty id;
	private StringProperty name;
	private StringProperty gender;
	private IntegerProperty birthYear;
	private StringProperty position;
	private StringProperty area;
	public Employee(Integer id, String name, String gender, Integer birthYear, String position, String area) {
		super();
		this.id.set(id);
		this.name.set(name);
		this.gender.set(gender);
		this.birthYear.set(birthYear);
		this.position.set(position);
		this.area.set(area);
	}
	public IntegerProperty getId() {
		return id;
	}
	public StringProperty getName() {
		return name;
	}
	public StringProperty getGender() {
		return gender;
	}
	public IntegerProperty getBirthYear() {
		return birthYear;
	}
	public StringProperty getPosition() {
		return position;
	}
	public StringProperty getArea() {
		return area;
	}
	
	
}
